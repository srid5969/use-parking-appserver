{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "PATCH", "HEAD"]
    }
  ],
  "crons": [
    {
      "path": "/cron/send-reminder-to-join-call",
      "schedule": "*/10 * * * *"
    }
  ]
}
